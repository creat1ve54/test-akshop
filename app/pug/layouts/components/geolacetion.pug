mixin geolacetion
    //- .geolacetion
        .geolacetion__left
            .geolacetion__left-caption
                .geolacetion__left-title Выберите способ получения
            .geolacetion__left-change
                .geolacetion__left-change-btn.geolacetion__left-change-btn--active
                    input(id="change1" type="radio" name="changeGeolacetion" value="Доставка" checked)
                    label( for="change1").btn Доставка
                .geolacetion__left-change-btn
                    input(id="change2" type="radio" name="changeGeolacetion" value="Самовывоз")
                    label(for="change2").btn Самовывоз
            .custom-input.custom-input--active.geolacetion__left-input.geolacetion__left-input--active
                //- .custom-input__container.geolacetion__left-input-container
                .popup__header.mb-16
                    button(type="button").btn-back
                    h3 Введите город, улицу, дом
                .custom-input
                    input(type="text" required).custom-input__text.geolacetion__left-input-text
                    label.custom-input__placeholder.geolacetion__left-input-placeholder Город,улица,дом
                    .custom-input__clear.geolacetion__left-input-img
                        //- img(src="/assets/images/clear_input.svg" alt="Clear")
                        //- .custom-input__error.geolacetion__left-input-error Необходимо указать адрес с точностью до номера дома
                        //- .custom-input__list
                            ul.custom-input__items.custom-scroll
                                li.custom-input__item
                                    .custom-input__item-up ул. Рашпилевская, 22
                                    .custom-input__item-down
                                        .custom-input__item-city Краснодарский край, г. Новороссийск
                                li.custom-input__item
                                    .custom-input__item-up ул. Рашпилевская, 22
                                    .custom-input__item-down
                                        .custom-input__item-city Краснодарский край, г. Новороссийск
                                li.custom-input__item
                                    .custom-input__item-up ул. Рашпилевская, 22
                                    .custom-input__item-down
                                        .custom-input__item-city Краснодарский край, г. Новороссийск
                                li.custom-input__item
